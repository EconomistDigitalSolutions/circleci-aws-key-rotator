{
  "name": "key-rotate",
  "version": "1.0.0",
  "description": "CircleCI Key Rotation",
  "main": ".build/src/handler.js",
  "scripts": {
    "clean": "rm -rf .build && rm -rf .serverless && rm -rf lambda.zip",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "npm run lint && tsc",
    "clean-build": "npm run clean && npm run build",
    "test": "jest"
  },
  "author": "Dom Parfitt",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.374.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.15",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.14",
    "@types/promise.prototype.finally": "^2.0.2",
    "aws-sdk-mock": "^4.3.0",
    "jest": "^23.6.0",
    "serverless": "^1.35.0",
    "serverless-offline": "^3.31.3",
    "serverless-plugin-typescript": "^1.1.5",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**"
    ]
  }
}
